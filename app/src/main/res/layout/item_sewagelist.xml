<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/llSewageList"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dip_35"
    android:orientation="horizontal">
        <TextView
            android:id="@+id/tvName"
            android:gravity="center"
            android:text="罗懂"
            android:layout_width="0dip"
            android:background="@drawable/bg_f3f4f6_lefit5"
            android:textColor="@color/text_color_999999"
            android:textSize="@dimen/fond_12sp"
            android:layout_height="match_parent"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/tvChushuiliang"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@drawable/bg_ffffff_right5"
            android:text="0"
            android:textColor="@color/text_color_333"
            android:textSize="@dimen/fond_12sp" />

        <TextView
            android:id="@+id/tvCOD"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="9999"
            android:background="@drawable/bg_ffffff_right5"
            android:textColor="@color/text_color_333"
            android:textSize="@dimen/fond_12sp"  />

        <TextView
            android:id="@+id/tvPH"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="99"
            android:background="@drawable/bg_ffffff_right5"
            android:textColor="@color/text_color_333"
            android:textSize="@dimen/fond_12sp"  />

        <TextView
            android:id="@+id/tvZonglin"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="999"
            android:background="@drawable/bg_ffffff_right5"
            android:textColor="@color/text_color_333"
            android:textSize="@dimen/fond_12sp" />

        <TextView
            android:id="@+id/tvZongan"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@drawable/bg_ffffff_right5"
            android:text="999"
            android:textColor="@color/text_color_333"
            android:textSize="@dimen/fond_12sp"  />
    </LinearLayout>